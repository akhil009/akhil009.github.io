<script>
var image = document.getElementById("aaa");
var image1 = document.getElementById("bbb");

var img_array=["images12.jpg","images12.jpg","images13.jpeg","images13.jpeg"];

var index=0;
function slide()
{

    document["aaa"].src = img_array[index];
    document["bbb"].src = img_array[index];

    if(index==0)
    {
      document["aaa"].style.left = -770;
       document["aaa"].style.top = 100;
      document["bbb"].style.left = -200;
      document["bbb"].style.top = 100;
        

     }
    else  if(index==1)
   {  document["aaa"].style.left = -600;
      document["aaa"].style.top = 100;
      document["bbb"].style.left = -30;
      document["bbb"].style.top = 100;
      
    }
    else  if(index==2)
   { document["aaa"].style.left = -770;
        document["aaa"].style.top = 300;
     document["bbb"].style.left = -200;
      document["bbb"].style.top = 300;
    }
    else  if(index==3)
   { document["aaa"].style.left = -600;
        document["aaa"].style.top = 300;
     document["bbb"].style.left = -30;
      document["bbb"].style.top = 300;
    }
    index++;
    if(index>=img_array.length)
    {
    index=0;
    }
}
setInterval("slide()",50);

</script>

